depend() {
	need net
}

start() {
	ebegin "Starting tac_plus"
	start-stop-daemon --start --quiet --exec /usr/sbin/tac_plus \
		-- -b -p ${PID} ${CFG}
	eend $?
}

stop() {
	ebegin "Stopping tac_plus"
	start-stop-daemon --stop --quiet --pidfile ${PID}
	eend $?
}
